name: mlflow_mockup

conda_env: environment.yaml

entry_points:
  main:
    parameters:
      alpha: float
      l1_ratio: {type: float, default: 0.1}
      brand: str
      model_version: float
    command: "python3 -W ignore::DeprecationWarning train.py -a {alpha} -l1 {l1_ratio} -brand {brand} -mversion {model_version}"
